
<%= render 'admin/header' %>

<h1>Listing admin_news</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_news.each do |admin_news| %>
  <tr>
    <td><%= admin_news.id %></td>
    <td><%= admin_news.name %></td>
    <td><%= admin_news.content %></td>
    <td><%= link_to 'Show', admin_news %></td>
    <td><%= link_to 'Edit', edit_admin_news_path(admin_news) %></td>
    <td><%= link_to 'Destroy', admin_news, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New News', new_admin_news_path %>
